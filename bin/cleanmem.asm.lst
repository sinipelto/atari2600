      1  0000					      processor	6502	; define used processor chip
      2  0000 ????
      3  0000 ????				      seg	code	; segment "code"
      4  f000					      org	$F000	; start address
      5  f000
      6  f000				   Start
      7  f000		       78		      sei		; disable interrupts flag = 0
      8  f001		       d8		      cld		; clear decimal flag
